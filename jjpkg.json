{
  "compile_entry": "app.go",
  "app_info": {
    "name": "blog",
    "id": "app_blog",
    "version": "6.9",
    "description": "go blog with vue"
  },
  "workdir": {
    "root": "/home/apps",
    "log": "."
  },
  "config": {
    "port": [10002, 10003]
  },
  "cmd": {
    "start": "./app_blog -s start",
    "stop": "./app_blog -s stop",
    "restart": "./app_blog -s restart",
    "status": "./app_blog -s status"
  }
}
